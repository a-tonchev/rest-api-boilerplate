{
  "name": "rest-api-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "Author": "Anton Tonchev",
  "license": "LicenseRef-LICENSE",
  "private": true,
  "type": "module",
  "imports": {
    "#lib/*": "./src/lib/*",
    "#modules/*": "./src/modules/*"
  },
  "exports": {
    "import": "./index.js",
    "require": "./index_cjs.cjs"
  },
  "engines": {
    "npm": ">=10.1.2",
    "node": ">=20.11.1"
  },
  "dependencies": {
    "ajv": "^8.6.2",
    "ajv-draft-04": "^1.0.0",
    "bcrypt": "^6.0.0",
    "esm": "^3.2.25",
    "express-useragent": "^2.1.0",
    "ipaddr.js": "^2.0.1",
    "lodash-es": "^4.17.21",
    "mongo-dot-notation": "^3.1.0",
    "mongodb": "^7.1.0",
    "nodemon": "^3.0.1",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.58.0"
  },
  "scripts": {
    "start": "node --import ./register.mjs index.js",
    "start-dev": "environment=local ./node_modules/nodemon/bin/nodemon.js --import ./register.mjs index.js -q",
    "start-dev-inspect": "environment=local ./node_modules/nodemon/bin/nodemon.js --inspect --import ./register.mjs index.js -q",
    "lint": "eslint src/**/*.js startup/**/*.js --fix",
    "createLib": "node scripts/libTemplate/createLib.cjs -o"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "eslint": "^9.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.23.4",
    "fs-extra": "^11.1.1",
    "yargs": "^18.0.0"
  }
}
